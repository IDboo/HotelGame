<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HotelGame.WebMVC</title>
    
    @await Html.PartialAsync("_UserCssPartialView")

</head>
<body>

    <partial name="_UserHeaderPartialView" />


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <partial name="_UserFooterPartialView" />

    @await Html.PartialAsync("_UserJsPartialView");

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
