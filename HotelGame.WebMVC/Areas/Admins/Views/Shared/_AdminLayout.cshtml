<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HotelGame.WebMVC</title>
    
    @await Html.PartialAsync("_AdminCssPartialView")

</head>
<body id="page-top">

    <script src="~/lib/jquery/dist/jquery-3.3.1.js"></script>

    <partial name="_AdminHeaderPartialView" />



    <partial name="_AdminSideBarPartialView" />

    <main role="main" class="main">
        <div class="pagetitle">
            @RenderBody()
        </div>
    </main>

    <partial name="_AdminFooterPartialView" />

    @await Html.PartialAsync("_AdminJsPartialView");

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
